  namespace Apps {
    AppList =				cds enc dc
    ScriptAppList =			tcl py
    namespace dc {
      TemplateDir =	$::Sshaft::SshaftTechDir/dc
      InitSrcFiles = 	synopsys_dc.setup synopsys_dc.setup.tcl \
			synopsys_pt.setup synopsys_pt.setup
      InitDestFiles = 	.synopsys_dc.setup .synopsys_dc.setup.tcl \
			.synopsys_pt.setup .synopsys_pp.setup
      Environment =	synopsys
      CmdParseRegExp = 	^(.*)$$
      CmdLogFormat =	%s -log %s -la -ll %d -li %d -tol %s
      CmdNoLogFormat =	%s -ll %d -li %d -tol %s
      TestCmd =		substep3
      Language = 	tcl
      ExeHeaderFile =	exe.hdr
      ExeFooterFile = 	exe.ftr
      ExecuteCommand =	dc_shell-t -f exe.tcl
      LogCapture =	dc.out
      LogCheck = 	dc.out
      LogCheckRegExp =	Error
      LogCheckMode = 	fail
    }
    namespace pt {
      TemplateDir =	$::Sshaft::SshaftTechDir/dc
      InitSrcFiles = 	synopsys_dc.setup synopsys_dc.setup.tcl \
			synopsys_pt.setup synopsys_pt.setup
      InitDestFiles = 	.synopsys_dc.setup .synopsys_dc.setup.tcl \
			.synopsys_pt.setup .synopsys_pp.setup
      Environment =	synopsys_tools
      CmdParseRegExp = 	^(.*)$$
      CmdLogFormat =	%s -log %s -la -ll %d -li %d -tol %s
      CmdNoLogFormat =	%s -ll %d -li %d -tol %s
      TestCmd =		substep3
      Language = 	tcl
      ExeHeaderFile =	exe.hdr
      ExeFooterFile = 	exe.ftr
      ExecuteCommand =	pt_shell -f exe.tcl
      LogCapture =	pt.out
      LogCheck = 	pt.out
      LogCheckRegExp =	Error
      LogCheckMode = 	fail
    }
    namespace pp {
      TemplateDir =	$::Sshaft::SshaftTechDir/dc
      InitSrcFiles = 	synopsys_dc.setup synopsys_dc.setup.tcl \
			synopsys_pt.setup synopsys_pt.setup
      InitDestFiles = 	.synopsys_dc.setup .synopsys_dc.setup.tcl \
			.synopsys_pt.setup .synopsys_pp.setup
      Environment =	synopsys_tools
      CmdParseRegExp = 	^(.*)$$
      CmdLogFormat =	%s -log %s -la -ll %d -li %d -tol %s
      CmdNoLogFormat =	%s -ll %d -li %d -tol %s
      TestCmd =		substep3
      Language = 	tcl
      ExeHeaderFile =	exe.hdr
      ExeFooterFile = 	exe.ftr
      ExecuteCommand =	pp_shell -f exe.tcl
      LogCapture =	pp.out
      LogCheck = 	pp.out
      LogCheckRegExp =	Error
      LogCheckMode = 	fail
    }
    namespace enc {
      TemplateDir =	$::Sshaft::SshaftTechDir/enc
      InitSrcFiles = 	encrc add_cadence
      InitDestFiles = 	.encrc add_cadence
      Environment = 	cadence
      CmdParseRegExp = 	^(.*)$$
      CmdLogFormat =	set errorflag [catch {%s -log %s -la -ll %d -li %d -tol %s} retval]
      CmdNoLogFormat =	set errorflag [catch {%s -ll %d -li %d -tol %s} retval]
      TestCmd =		substep3
      Language = 	tcl
      ExeHeaderFile =	exe.hdr
      ExeFooterFile = 	exe.ftr
      ExecuteCommand =	encounter -log encounter.log -nowin -overwrite \
			-replay exe.tcl
      LogCapture = 	encounter.out
      LogCheck = 	encounter.log
      LogCheckRegExp =	\*\*ERROR
      LogCheckMode = 	fail
    }
    namespace cds {
      TemplateDir =	$::Sshaft::SshaftTechDir/cds
      InitSrcFiles = 	cdsinit cds.lib lib.defs
      InitDestFiles = 	.cdsinit cds.lib lib.defs
      Environment = 	cadence
      CmdParseRegExp = 	^(.*)\)\s*$$
      CmdLogFormat =	%s ?stepargs list( "%s" t %d %d "%s"))
      CmdNoLogFormat =	%s ?stepargs list( nil nil %d %d "%s"))
      TestCmd =		substep3()
      Language = 	il
      ExeHeaderFile =	exe.hdr
      ExeFooterFile = 	exe.ftr
      ExecuteCommand =	icfb -log CDS.log -nograph -replay exe.il
      LogCapture = 	icfb.out
      LogCheck = 	CDS.log
      LogCheckRegExp =	^\\t t
      LogCheckMode = 	pass      
    }
    namespace oa {
      TemplateDir =	$::Sshaft::SshaftTechDir/oa
      InitSrcFiles = 	cds.lib
      InitDestFiles = 	cds.lib
      Environment = 	oa
      CmdParseRegExp = 	^(.*)\)\s*$$
      CmdLogFormat =	%s ?stepargs list( "%s" t %d %d "%s"))
      CmdNoLogFormat =	%s ?stepargs list( nil nil %d %d "%s"))
      TestCmd =		substep3
      Language = 	py
      ExeHeaderFile =	exe.hdr
      ExeFooterFile = 	exe.ftr
      ExecuteCommand =	icfb -log CDS.log -nograph -replay exe.il
      LogCapture = 	icfb.out
      LogCheck = 	CDS.log
      LogCheckRegExp =	^\\t t
      LogCheckMode = 	pass      
    }
    namespace tcl {
      Path =				/usr/local/bin
      Executable = 			tclsh
    }
  }
